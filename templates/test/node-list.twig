<style type="text/css">
*{ font-size: 25px;}
</style>
{% if nodes %}
<table border="1" cellpadding="0" cellspacing="0">
    <tr>
        <th>id</th>
        <th>name</th>
        <th>created_at</th>
        <th>updated_at</th>
        <th>delete</th>
    </tr>
    {% for node in nodes %}
    <tr>
        <td>{{ node.id }}</td>
        <td>{{ node.node_name }}</td>
        <td>{{ node.node_desc }}</td>
        <td>{{ node.operator }}</td>
        <td>{{ node.controls }}</td>
        <td>{{ node.created_at }}</td>
        <td>{{ node.updated_at }}</td>
        <td>
            <a href="{{ path_for('api_delete_node', {id: node.id}) }}">&nbsp;&lt;&lt;delete</a>
        </td>
    </tr>
    {% endfor %}
    </table>
{% else %}
    <h1>empty</h1>
{% endif %}